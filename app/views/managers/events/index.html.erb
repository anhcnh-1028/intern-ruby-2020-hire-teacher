<%= provide :title, t(".title") %>
<div class="row">
  <div class="col-lg-12">
    <div class="row page-header">
      <div class="col-lg-6">
        <h1>
          <%= t ".title" %>
        </h1>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-11">
        <div class="row first">
          <%= render "managers/events/search" %>
        </div>
        <%= render "managers/events/table", events: @events %>
      </div>
    </div>
  </div>
</div>
