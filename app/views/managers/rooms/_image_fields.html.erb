<div class="form-group image-container">
  <label><%= t ".image" %></label>
  <%= f.fields_for :images do |i| %>
    <div class="flex-box first">
      <%= i.file_field :image, class: "form-control-file"%>
    </div>
  <% end %>
</div>
<div class="form-group">
  <button type="button" id="add_js_btn" class="btn bg-transparent">
    <i class="fa fa-plus fa-lg"></i>
  </button>
</div>
